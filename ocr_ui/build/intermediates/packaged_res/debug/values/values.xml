<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <dimen name="camera_bottom_margin">16dp</dimen>
    <dimen name="camera_left_margin">18dp</dimen>
    <dimen name="camera_right_margin">18dp</dimen>
    <dimen name="camera_top_margin">16dp</dimen>
    <string name="app_name">OCR_UI</string>
    <string name="camera_permission_required">本功能需要相机权限！</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <declare-styleable name="OCRCameraLayout">
        <attr format="reference" name="contentView"/>
        <attr format="reference" name="centerView"/>
        <attr format="reference" name="leftDownView"/>
        <attr format="reference" name="rightUpView"/>
    </declare-styleable>
</resources>